<Window x:Class="GenCamFB.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GenCamFB"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical">
            <StackPanel Orientation="Horizontal">
                <TextBlock Text="Cam Json File" MinWidth="100"/>
                <TextBox MinWidth="100"/>
                <Button Content="..." Click="LoadButton_Click" MinWidth ="50" Name="LoadButton"/>
            </StackPanel>
            <StackPanel Orientation="Horizontal">
                <TextBlock Text="Cam FB Xml" MinWidth="100"/>
                <TextBox MinWidth="100"/>
                <Button  Content="..."  Click="SaveButton_Click" MinWidth ="50" Name="SaveButton"/>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
